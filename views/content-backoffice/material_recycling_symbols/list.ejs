<% include ../../template-backoffice/header.ejs %>

<!-- BEGIN PAGE CONTENT -->
<div class="page-content">
  <div class="header">
    <h2><strong>Management</strong>&nbsp;Recycling Symbols</h2>
    <div class="breadcrumb-wrapper">
      <ol class="breadcrumb">
        <li><a href="/">Dashboard</a>
        </li>
        <li class="active">Management Recycling Symbols</li>
      </ol>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <div class="panel">
        <div class="panel-header header-line">
           <a class="btn btn-primary btn-embossed" href="/material_recycling_symbols/insert" role="button">Input Data</a>
        </div>
        <div class="panel-content">
          <table class="table table-striped" id="thisdata">
            <thead>
              <tr>
                <th>No.</th>
                <th>Recycling Symbols Name</th>
                <th>Description</th>
                <th>Recycling Symbols Image</th>
                <th>Action</th>
              </tr>
            </thead>
            
            <tbody>
              <% for(var i=0; i<data.length; i++){ %>
              <tr>
                <td><%= i+1 %>.</td>
                <td><%= data[i]['symbol_name'] %></td>
                <td><%= data[i]['deskripsi'] %></td>
                <td><img style="height:100px;widht:100" src="/foto/<%= data[i]['foto'] %>"></td>
                <td>
                  <ul class="list-inline">
                    <li><a class="btn btn-warning btn-embossed btn-sm" href="/material_recycling_symbols/edit/<%= data[i]['id'] %>" role="button" ><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a></li>

                    <li><a class="btn btn-danger btn-embossed btn-sm" href="/material_recycling_symbols/delete/<%= data[i]['id'] %>" role="button"><i class="fa fa-trash-o" aria-hidden="true"></i></a></li>
                  </ul>
                </td>
              </tr>
              <% } %>
              <!-- <tr>
                <td>1.</td>
                <td>Polyethylene Terephthalate(PET)</td>
                <td>Description PET is clear, tough, and has good gas and moisture barrier properties. Commonly used in soft drink bottles and both food and non-food containers.</td>
                <td><img src="http://placehold.it/125x160"></td>
                <td class="text-center">
                  <a class="edit btn btn-warning btn-embossed btn-sm" href="/material_recycling_symbols/edit"><i class="icon-note"></i></a>
                  <a class="delete btn btn-danger btn-embossed btn-sm" href=""><i class="icons-office-52"></i></a>
                </td>
              </tr> -->
            </tbody>
          </table>
        </div>
      </div>
    </div>  
  </div>
  <div class="footer">
    <div class="copyright">
      <p class="pull-left sm-pull-reset">
        <span>Copyright <span class="copyright">Â©</span> 2016 </span>
        <span>PT Emory Langgengjaya</span>.
      </p>
    </div>
  </div>
</div>
<!-- END PAGE CONTENT -->

<% include ../../template-backoffice/footer.ejs %>

<script type="text/javascript">
  $(document).ready(function() {
      $('#thisdata').DataTable({
        columnDefs: [
            { width: 50, targets: 0 },
            { width: 100, targets: 2 }
        ],
      });
  } );
</script>